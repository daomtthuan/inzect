{
  "catppuccin.accentColor": "sapphire",
  "catppuccin.colorOverrides": {
    "mocha": {
      "base": "#11111b",
      "blue": "#89b4fa",
      "crust": "#1e1e2e",
      "flamingo": "#f2cdcd",
      "green": "#a6e3a1",
      "lavender": "#b4befe",
      "mantle": "#181825",
      "maroon": "#eba0ac",
      "mauve": "#cba6f7",
      "overlay0": "#6c7086",
      "overlay1": "#7f849c",
      "overlay2": "#9399b2",
      "peach": "#fab387",
      "pink": "#f5c2e7",
      "red": "#f38ba8",
      "rosewater": "#f5e0dc",
      "sapphire": "#74c7ec",
      "sky": "#89dceb",
      "subtext0": "#a6adc8",
      "subtext1": "#bac2de",
      "surface0": "#313244",
      "surface1": "#45475a",
      "surface2": "#585b70",
      "teal": "#94e2d5",
      "text": "#cdd6f4",
      "yellow": "#f9e2af"
    }
  },
  "catppuccin.italicComments": true,
  "catppuccin.italicKeywords": true,
  "catppuccin.workbenchMode": "flat",
  "codemetrics.basics.CodeLensHiddenUnder": 0,
  "codemetrics.basics.ComplexityTemplate": "Complexity: {0}",
  "codemetrics.basics.DecorationModeEnabled": false,
  "cSpell.caseSensitive": true,
  "cSpell.words": ["inzect", "ThuanDMT", "Daomtthuan"],
  "dotenv.cloakColor": "#585b70",
  "dotenv.cloakIcon": "*",
  "dotenv.enableAutocloaking": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "CartographCF Nerd Font, Cartograph CF, Cascadia Code NF, Cascadia Code PL, Source Code Pro, Consolas, Courier New, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13.5,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": "active",
  "editor.lineHeight": 1.6,
  "editor.linkedEditing": true,
  "editor.rulers": [160],
  "editor.stickyScroll.enabled": false,
  "explorer.compactFolders": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*": "${capture}.map",
    "*.js": "${capture}.js.map, ${capture}.cjs, ${capture}.mjs, ${capture}.*.map, ${capture}.d.ts",
    "*.ts": "${capture}.type.ts",
    "*.tsx": "${capture}.type.ts",
    ".env": ".env*",
    "package.json": ".editorconfig, .git*, pnpm-*.yaml, tsconfig.*, vite.config.*, prettier.config.*, eslint.config.*, swc.config.*, rollup.config.*, electron-builder.*, components.*, index.html"
  },
  "files.associations": {
    "**/tsconfig/*.json": "jsonc",
    "*.config.json": "jsonc",
    ".env*": "dotenv"
  },
  "files.autoSave": "onFocusChange",
  "files.eol": "\n",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/release": true,
    "**/Thumbs.db": true
  },
  "files.readonlyInclude": {
    "**/dist": true,
    "**/node_modules": true,
    "**/release": true
  },
  "git.autofetch": true,
  "git.blame.editorDecoration.enabled": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.runMode": "on-demand",
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "markdown.extension.toc.levels": "2..4",
  "prettier.enableDebugLogs": true,
  "prettier.endOfLine": "lf",
  "sort-json.settings.excludePaths": ["tsImportSorter.configuration.groupRules"],
  "sort-json.settings.sortMode": "Objects Only",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.suggest.enabled": true,
  "tsImportSorter.configuration.groupRules": [
    // # Others
    {
      "flags ": ""
    },
    // # Scripts
    {
      "flags": "scripts",
      "subGroups": [
        // # Dependencies
        {
          "regex": "^(?!(\\.))",
          "subGroups": ["^@", "^[^@]"]
        },
        // # Modules
        {
          "regex": "^(?=(~))"
        },
        // # Relative
        {
          "regex": "^(?=(\\.))"
        }
      ]
    },
    // # Types
    {
      "importType": true,
      "subGroups": [
        // # Dependencies
        {
          "regex": "^(?!(\\.))",
          "subGroups": ["^@", "^[^@]"]
        },
        // # Modules
        {
          "regex": "^(?=(~))"
        },
        // # Relative
        {
          "regex": "^(?=(\\.))"
        }
      ]
    },
    // # Modules
    {
      "subGroups": [
        // # Dependencies
        {
          "regex": "^(?!(\\.))",
          "subGroups": ["^@", "^[^@]"]
        },
        // # Modules
        {
          "regex": "^(?=(~))"
        },
        // # Relative
        {
          "regex": "^(?=(\\.))"
        }
      ]
    }
  ],
  "tsImportSorter.configuration.wrappingStyle": "prettier",
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.implementationsCodeLens.showOnInterfaceMethods": true,
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.startupEditor": "none",
  "workbench.tree.enableStickyScroll": false,
  "markdownlint.config": {
    "default": true,
    "MD024": { "siblings_only": true }
  }
}
